{
  "name": "date-time-it",
  "version": "1.0.0",
  "description": "A data plugin that generates random dates and times in italian format",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Date & Time IT",
    "manifest": "src/manifest.json",
    "main": "date-time-it.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "repository": {
    "url": "https://github.com/massimo-cassandro/sketch-date-time-it-plugin"
  },
  "author": "Massimo Cassandro",
  "license": "MIT",
  "devDependencies": {
    "@skpm/builder": "^0.8.0"
  }
}
